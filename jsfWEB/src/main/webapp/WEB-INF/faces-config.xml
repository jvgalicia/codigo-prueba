	<application>
		<el-resolver>org.springframework.web.jsf.el.SpringBeanFacesELResolver</el-resolver>
		<resource-bundle>
			<base-name>META-INF.i18n.mensajesLog_es_ES</base-name>
			<var>es</var>
		</resource-bundle>
	</application>

	<!-- Configuracion para los Usuarios -->
	<managed-bean>
		<managed-bean-name>dominioUsuarios</managed-bean-name>
		<managed-bean-class>com.tecnides.dominio.UsuarioDTO</managed-bean-class>
		<managed-bean-scope>none</managed-bean-scope>
	</managed-bean>

	<managed-bean>
		<managed-bean-name>beanUser</managed-bean-name>
		<managed-bean-class>com.tecnides.jsfweb.bean.BeanUser</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
		<managed-property>
			<property-name>usuarios</property-name>
			<value>#{dominioUsuarios}</value>
		</managed-property>
		<managed-property>
			<property-name>servicioUser</property-name>
			<value>#{servicioUsuarioInj}</value>
		</managed-property>
		<managed-property>
			<property-name>servicioCatalogos</property-name>
			<value>#{interfaceCatalogos}</value>
		</managed-property>
	</managed-bean>
	<!-- Configuracion para beanAdministradora -->
	<managed-bean>
		<managed-bean-name>beanAdministradora</managed-bean-name>
		<managed-bean-class>com.tecnides.jsfweb.bean.BeanAdministradora</managed-bean-class>
